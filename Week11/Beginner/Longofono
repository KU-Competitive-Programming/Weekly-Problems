
public class Beginner_9 {
	public static void main(String [] args){
	int start = 1000;
	System.out.println("500");
	first(start);
	}
	
	public static void first(int a){
		System.out.println(a + " ");
		second(a);
	}
	public static void second(int b){
		System.out.println(1000-b + " ");
		b--;
		int hack = (1/(500-b));
		first(b);
	}
}

